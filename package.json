{
  "name": "@paxcode-it/zodify-forms-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "zodify-forms monorepo with Turbo",
  "author": "Wojciech Kr√≥l <wojciechkrol97@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paxcode-it/zodify-forms.git",
    "bugs": "https://github.com/paxcode-it/zodify-forms/issues"
  },
  "scripts": {
    "prebuild": "npx only-allow pnpm",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "build:apps": "turbo run build",
    "test:lib": "turbo run test --no-cache --force",
    "lint:lib": "turbo run lint",
    "build:lib": "turbo run build",
    "deploy:lib": "turbo run deploy",
    "pg": "turbo run dev --filter=@apps/playground"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "29.5.5",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.1.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "install": "^0.13.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-ts-webcompat-resolver": "1.0.0",
    "lint-staged": "14.0.1",
    "rimraf": "5.0.1",
    "semantic-release": "22.0.12",
    "semantic-release-monorepo": "^8.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "tsup": "8.1.0",
    "turbo": "^2.0.6",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
