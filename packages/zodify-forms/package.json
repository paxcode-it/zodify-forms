{
  "name": "@paxcode-it/zodify-forms",
  "private": true,
  "version": "0.0.1",
  "description": "Turn your Zod schemas into configurable field arrays for easy integration with HTML, React, Vue, and more.",
  "main": "lib/index.mjs",
  "module": "lib/index.mjs",
  "author": "Wojciech Kr√≥l <wojciechkrol97@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:wojtekKrol/zodify-forms",
    "bugs": "https://github.com/paxcode-it/zodify-forms/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.mjs",
        "types": "./lib/index.d.mts"
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "dynamic-fields",
    "field-generator",
    "fields",
    "react",
    "schema",
    "type-safe",
    "typescript",
    "validation",
    "zod"
  ],
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsup",
    "lint": "pnpm lint:code && pnpm lint:text",
    "lint:code": "eslint --cache --cache-location node_modules/.cache/eslint --cache-strategy content --ext ts .",
    "lint:text": "prettier --check \"**/*.{json,md,mdx,yml}\"",
    "test": "jest",
    "deploy": "semantic-release"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "validator": "13.12.0"
  }
}
